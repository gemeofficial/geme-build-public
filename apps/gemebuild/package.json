{
  "name": "gemebuild",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run storybook",
    "dev:next": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "sb dev -p 9007",
    "build-storybook": "sb build",
    "ci:test-storybook": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpm run build-storybook && http-server storybook-static --port 6006 --silent\" \"wait-on http://127.0.0.1:6006 && test-storybook\""
  },
  "main": "./src/i18n-pages/index.ts",
  "types": "./src/i18n-pages/index.ts",
  "peerDependencies": {
    "next": "14.2.3",
    "next-intl": "3.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "clsx": "^2.0.0",
    "d3-fetch": "^3.0.1",
    "d3-scale": "^4.0.2",
    "lodash.sortby": "^4.7.0",
    "mixpanel-browser": "^2.47.0",
    "react-simple-maps": "^3.0.0",
    "swiper": "^10.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.6.13",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.13",
    "@storybook/cli": "^7.6.13",
    "@storybook/nextjs": "^7.6.13",
    "@storybook/react": "^7.4.0",
    "@storybook/test": "^7.6.13",
    "@storybook/test-runner": "^0.16.0",
    "@types/d3-fetch": "^3.0.3",
    "@types/d3-scale": "^4.0.4",
    "@types/lodash.sortby": "^4.7.7",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-next": "^14.2.3",
    "http-server": "^14.1.1",
    "next": "14.2.3",
    "next-intl": "3.12.0",
