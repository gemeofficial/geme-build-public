{
  "name": "workshop",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "storybook dev -p 9009",
    "build": "storybook build",
    "start": "serve storybook-static",
    "clean": "rm -rf .turbo && rm -rf node_modules",
    "lint": "eslint ./src/stories/**/*.stories.tsx",
    "ci:test-storybook": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpm run build && http-server storybook-static --port 6006 --silent\" \"wait-on http://127.0.0.1:6006 && test-storybook\""
  },
  "peerDependencies": {
    "next": "13.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "next": "13.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.6.13",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.13",
    "@storybook/cli": "^7.6.13",
    "@storybook/nextjs": "^7.6.13",
    "@storybook/react": "^7.4.0",
    "@storybook/test": "^7.6.13",
    "storybook": "7.6.12",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.49.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-next": "^13.5.2",
    "postcss": "^8.4.35",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.3.3",
    "tsconfig": "workspace:*",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "5.2.2",
    "ui": "workspace:*",
    "@storybook/test-runner": "^0.16.0",
    "concurrently": "^8.2.2",
    "http-server": "^14.1.1",
    "wait-on": "^7.2.0"
  }
}
